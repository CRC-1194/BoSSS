<!-- BoSSS control file, specialized for the compressible Navier-Stokes solver 
     Viscous Shock Profile TestCase by Iannelli with PowerLaw for viscosity -->
    
<BoSSSControl>
	<Base>
		<dboptions path="c:\bosss_dbv2\VSP_M4_PowerLaw_unsteady\" />
		<!-- no grid
    <grid guid="00000000-0000-0000-0000-000000000128" />-->

    <!-- grid guids
    Subsonic Outflow
    h=1/32  => 3475547b-cef3-43df-8794-cacf6593066f
    h=1/64  => 8be585f8-ffe9-4908-90bf-9d47603d54d5
    h=1/128 => 3eeea53f-3084-427e-a38c-d422cd8e10b0
    
    tStart=1, mu=Const
    h=4/32 => b7031529-260d-4c4f-b2aa-6c03c6172f41
    h=4/64 => 6b2a511f-d7a9-45d7-8739-473bc7d03a76
    h=4/128 => b6af61a4-29d5-4783-9e47-cab6df6eedc9
    -->

    <grid guid="6b2a511f-d7a9-45d7-8739-473bc7d03a76"/>

    <description name="M2_h=1/64_p=1_RK=1_CFL=0.01">
			viscous NSE Test -> ViscousShockProfile
		</description>
		
		<tags>
			viscous, NSE, VSP
		</tags>
    <!-- Specify "solver" or "query" or "parameterStudy" -->
		<run mode="parameterStudy"/>
    
		<fields_degree>
			<!-- Specify DG polynomial degree; If degree for "u", "e", "p",
								"T", "Ma", "K" or "S" are given, these fields will also be
								calculated and included in IO operations -->
			<field	identification="rho"	degree="15" />
			<field	identification="m"		degree="15" />
			<field	identification="rhoE"	degree="15" />
      <field  identification="u" degree="15"/>
      <field  identification="p" degree="15"/>
		</fields_degree>
	</Base>

	<!-- Specify "values" or "restart" -->
	<initial mode="values">

    
		<restart>
      <!-- Restart session-guids (ref_Sol):
      
      Restart Sessions: Subsonic Outflow
      h=1/64  => 
      h=1/128 => 
      h=1/256 => 
      -->
			<session guid="d4bd33f7-025e-44d7-8d2f-e36bfb595386" timestepno="" />
		</restart>

		<values>
			<formula>
        rho(x) = 0.0
      </formula>
			<formula>
        m0(x) = 0.0
      </formula>
			<formula>
        rhoE(x) = 0.0
      </formula>
    </values>
	</initial>

	<Solver>
		<logging savetodb="true" saveperiod="10000" tracing="false"/>
		<termcond Endtime="1.0" NoOfTimesteps="20000000" />
		<timestepping dtMin="0" dtMax="0.01" CFLFrac="0.04"/>
		<!--<timestepping dtMin="4.82e-6" dtMax="4.82e-6" CFLFrac="0.01"/>-->

		<boundary>
			<bc edgetagname="supersonicinlet">
        <!--<formula>
          rho(x) =  1.001001001001001 + jump(x-0.5)*1.663001663001664
        </formula>
        <formula>
          u0(x) = 2.364065481326607- jump(x-0.5)*1.475766101894199
        </formula>
        <formula>
          p(x) = 1.002601801801802+ jump(x-0.5)*3.492302993402994
        </formula>-->
        <formula>
          rho(x) =  1.000000214255133
        </formula>
        <formula>
          u0(x) = 5.732862812439544
        </formula>
        <formula>
          p(x) = 1.000001585487833
        </formula>
      </bc>


			<bc edgetagname="subsonicoutlet">
        <value field="p">4.499999999999489</value>
        <!--<value field="p">4.5</value>-->
			</bc>

		</boundary>
		<properties>
		
			<string key="equationSystem">NavierStokes</string>
			<string key="convectiveFluxType">HLLC</string>
			
			<string key="diffusiveFluxType">SIPG</string>			
			<float key="SIPGPenaltyConstant">5.0</float> 

			<string key="timeStepping">explicit</string>
			<string key="explicitScheme">RungeKutta</string>
			<int key="explicitOrder">1</int>

      <float key="Reynolds">5.561347933278388</float>
			<float key="Mach">4.0</float>
			<float key="Prandtl">0.75</float>
			<float key="kappa">1.4</float>
			<!-- Choose "IdealGas", "Tait" or "StiffenedGas". Omitting this
								 option will make CNS default to "IdealGas".-->
			<string key="equationOfState">IdealGas</string>
      <!-- Choose "Constant", "Sutherland", "PowerLaw".
           Default is "Constant"-->
      <string key="viscosityLaw">PowerLaw</string>
			<!-- "None": No residual calculation;
								 "SteadyState": Residuals for a steady state problem;
								 "Unsteady": Residuals for an unsteady problem -->
			<string key="residualType">ChangeRate</string>
			<int key="residualInterval">1000</int>
      <!-- An optional list of termination criteria and the corresponding
                 thresholds. The criteria are based in the residual calculation
                 and have the following format: $residualType_$entityName
                 Here, the $entityName is either the name of a query or the
                 description of a particular residual for a particular field,
                 for example 'L2_abs_m0'. The individual criteria are combined
                 with a boolean AND combination i.e. CNS only terminates when
                 ALL criteria are fulfilled. -->
      <!--<string key="terminationCriteria">changeRate_L2_abs_rho, changeRate_L2_abs_m0</string>
      <fvector key="terminationThresholds">1e-14, 1e-14</fvector>-->
      <!-- Interval for 'Starting time step' statements (they can affect
                 serial and parallel performance of individual time-steps are
                 fast!). -->
      <int key="printInterval">1000</int>
			<!-- The following properties represent optional formulas defining 
								 source terms of the selected equation system. Each formula may
								 depend on independent variables as well as primal fields -->
			<!--<string key="rhoSource">f(x,rho) = rho*(x*x)</string>
						<string key="m0Source">f(x,m0) = m0*(x*x)</string>
						<string key="rhoESource">f(x,rho,m0,rhoE) = rho*m0*rhoE</string>-->
			<!-- Choose "Symbolic" or "byFlux" for the calculation of the
								 derivatives of the primal variables -->
			<string key="derivativeOption">None</string>
		</properties>


    
    <Queries>
			<L2Error id="SolL2Err_rho">
				<computedSolution>
					<field identification="rho" />
				</computedSolution>
				<exactSolution>
          <storedField TimestepGuid="cb714aec-4b4a-4dae-9e70-32861daa195f" FieldName="rho"/> 
				</exactSolution>
			</L2Error>
      <L2Error id="SolL2Err_m0">
        <computedSolution>
          <field identification="m0" />
        </computedSolution>
        <exactSolution>
          <storedField TimestepGuid="cb714aec-4b4a-4dae-9e70-32861daa195f" FieldName="m0"/>
        </exactSolution>
      </L2Error>
      <L2Error id="SolL2Err_rhoE">
        <computedSolution>
          <field identification="rhoE" />
        </computedSolution>
        <exactSolution>
          <storedField TimestepGuid="cb714aec-4b4a-4dae-9e70-32861daa195f" FieldName="rhoE"/>
        </exactSolution>
      </L2Error>
      <L2Error id="SolL2Err_U0">
        <computedSolution>
          <field identification="u0" />
        </computedSolution>
        <exactSolution>
          <storedField TimestepGuid="cb714aec-4b4a-4dae-9e70-32861daa195f" FieldName="u0"/>
        </exactSolution>
      </L2Error>
		</Queries>
    </Solver>
  <ParameterStudy logFileDirectory=".">
    <parameterSet>
      <parameter id="grid" xpath="Base/grid/@guid">
        b9d8fa7d-6a10-4b37-a1aa-6016ff11ef09
        e7832178-853c-4208-99ba-c8d440179596
        80ae1aee-ce8a-45b8-8984-307e37389480
      </parameter>
      <parameter id="description" xpath="Base/description/@name">
        M4_CFL=0.04_h=1/32_p=x
        M4_CFL=0.04_h=1/64_p=x
        M4_CFL=0.04_h=1/128_p=x
      </parameter>
      <parameter id="rho-query" xpath="Solver/Queries/L2Error[@id='SolL2Err_rho']/exactSolution/storedField/@TimestepGuid">
        703acb73-2912-4617-97a7-96db4e972ada
        2018538a-589b-4a26-9410-c0b31640f052
        fdd4b259-928f-4e80-b651-d4125f551a80
      </parameter>
      <parameter id="m0-query" xpath="Solver/Queries/L2Error[@id='SolL2Err_m0']/exactSolution/storedField/@TimestepGuid">
        703acb73-2912-4617-97a7-96db4e972ada
        2018538a-589b-4a26-9410-c0b31640f052
        fdd4b259-928f-4e80-b651-d4125f551a80
      </parameter>
      <parameter id="rhoE-query" xpath="Solver/Queries/L2Error[@id='SolL2Err_rhoE']/exactSolution/storedField/@TimestepGuid">
        703acb73-2912-4617-97a7-96db4e972ada
        2018538a-589b-4a26-9410-c0b31640f052
        fdd4b259-928f-4e80-b651-d4125f551a80
      </parameter>
      <parameter id="U0-query" xpath="Solver/Queries/L2Error[@id='SolL2Err_U0']/exactSolution/storedField/@TimestepGuid">
        703acb73-2912-4617-97a7-96db4e972ada
        2018538a-589b-4a26-9410-c0b31640f052
        fdd4b259-928f-4e80-b651-d4125f551a80
      </parameter>
    </parameterSet>
    <parameterSet>
      <parameter id="rho-degree" xpath="Base/fields_degree/field[@identification='rho']/@degree">1 2 3</parameter>
      <parameter id="m-degree" xpath="Base/fields_degree/field[@identification='m']/@degree">1 2 3</parameter>
      <parameter id="rhoE-degree" xpath="Base/fields_degree/field[@identification='rhoE']/@degree">1 2 3</parameter>
      <parameter id="u-degree" xpath="Base/fields_degree/field[@identification='u']/@degree">1 2 3</parameter>
      <parameter id="p-degree" xpath="Base/fields_degree/field[@identification='p']/@degree">1 2 3</parameter>
    </parameterSet>
  </ParameterStudy>

</BoSSSControl>
